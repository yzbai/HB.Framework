<?xml version="1.0" encoding="utf-8" ?>
<base:BaseContentPage
    x:Class="HB.FullStack.Mobile.TCaptcha.TCaptchaDialog"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:base="clr-namespace:HB.FullStack.Mobile.Base;assembly=HB.FullStack.Mobile"
    xmlns:baseControls="clr-namespace:HB.FullStack.Mobile.Controls;assembly=HB.FullStack.Mobile"
    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <base:BaseContentPage.Content>
        <StackLayout x:Name="ModalDialogContainer" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">

            <baseControls:HybridWebView x:Name="webView" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
        </StackLayout>
    </base:BaseContentPage.Content>
</base:BaseContentPage>