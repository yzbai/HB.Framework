<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>Latest</LangVersion>
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <AnalysisMode>AllEnabledByDefault</AnalysisMode>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Styles\materialdesignicons-webfont.ttf" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Styles\materialdesignicons-webfont.ttf" />
  </ItemGroup>

  <ItemGroup>
    
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Http.Polly" Version="5.0.1" />
    <PackageReference Include="Microsoft.Extensions.Logging.Debug" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="5.0.0" />
    <PackageReference Include="SkiaSharp.Extended" Version="1.60.0" />
    <PackageReference Include="SkiaSharp.Views.Forms" Version="2.80.2" />
    <PackageReference Include="Xamarin.CommunityToolkit" Version="1.0.0-pre6" />
    <PackageReference Include="Xamarin.Essentials" Version="1.6.1-preview1" />
    <PackageReference Include="Xamarin.Forms" Version="5.0.0.1874" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\HB.FullStack.Common\HB.FullStack.Common.csproj" />
    <ProjectReference Include="..\HB.FullStack.Database.ClientExtension\HB.FullStack.Database.ClientExtension.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Base\BaseModalDialog.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\ActionSheetCell.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\Chip.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\CommandCell.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\ImageOptions.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\MonthDayPicker.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\MultipleListPicker.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\MultiplePickerDialog.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\SingleListPicker.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Controls\SmsCodeEntry.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="TCaptcha\TCaptchaDialog.xaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="TCaptcha\TCaptchaDialog.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
  </ItemGroup>

</Project>
